{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard-with-typescript"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "project": "tsconfig.json",
    "ecmaVersion": "latest"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "@typescript-eslint/consistent-type-assertions": 0,
    "@typescript-eslint/no-debugger": 0,
    "@typescript-eslint/strict-boolean-expressions": 0
  },
  "ignorePatterns": ["src/authApp/*"]
}
